{
  "name": "countries-mern-api",
  "version": "1.0.0",
  "engines": {
    "node": "12.13.1"
  },
  "description": "Server countries-MERN requests",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Geoffrey Siele (sielekrisp@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.7",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "jest": "^25.1.0"
  }
}
